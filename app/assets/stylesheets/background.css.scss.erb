@font-face {
font-family: 'nexa';
src: url('<%= asset_path("fonts/nevis.ttf") %>'); /* IE9 Compat Modes */
src: url('<%= asset_path("fonts/nexabold.otf") %>'),
url('<%= asset_path("fonts/nexalight.otf") %>')
}

@font-face {
font-family: 'nexa-light';
src: url('<%= asset_path("fonts/nevis.ttf") %>'); /* IE9 Compat Modes */
src: url('<%= asset_path("fonts/nexalight.otf") %>')
}

.lines{
	background: url('<%= asset_path("bg.png") %>') repeat;
	position: absolute;
	width: 100%;
	height: 594px;
	z-inde: 1;
}

.top-header{
background: -moz-linear-gradient(45deg, rgba(255,255,255,0) 0%, rgba(151,156,163,0.92) 92%, rgba(151,156,163,1) 70%);
background: -webkit-linear-gradient(45deg, rgba(255,255,255,0) 0%, rgba(151,156,163,0.92) 92%, rgba(151,156,163,1) 100%);
background: -o-linear-gradient(45deg, rgba(255,255,255,0) 0%, rgba(151,156,163,0.92) 92%, rgba(151,156,163,1) 70%);
background: -ms-linear-gradient(45deg, rgba(255,255,255,0) 0%, rgba(151,156,163,0.92) 92%, rgba(151,156,163,1) 70%);
background: linear-gradient(90deg, rgba(247,245,245,0) 0%, rgba(247,245,245,0) 80%, rgba(247,245,245,0) 70%);
}



.mobile-mg{
