@font-face {
font-family: 'nexa';
src: url('<%= asset_path("fonts/nevis.ttf") %>'); /* IE9 Compat Modes */
src: url('<%= asset_path("fonts/nexabold.otf") %>'),
url('<%= asset_path("fonts/nexalight.otf") %>')
}

@font-face {
font-family: 'nexa-light';
src: url('<%= asset_path("fonts/nevis.ttf") %>'); /* IE9 Compat Modes */
src: url('<%= asset_path("fonts/nexalight.otf") %>')
}

.top-header{
  background: #e4e3e2;
  background-image: url('<%= asset_path("bg.png") %>') #e4e3e2,
    -webkit-gradient(to right, #fff, #e4e3e2),
    -webkit-linear-gradient(to left, #fff, #e4e3e2),
    -o-linear-gradient(to right, #fff, #e4e3e2),
    linear-gradient(to right, #fff, #e4e3e2)
}


